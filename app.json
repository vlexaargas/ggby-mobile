{
  "expo": {
    "sdkVersion": "33.0.0",
    "privacy": "public",
    "name": "GGBY Guide",
    "icon": "./assets/appicon/ggby-logo-ios.png",
    "version": "1.4.4",
    "orientation": "default",
    "slug": "ggby-guide",
    "primaryColor": "#252A33",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F4F4"
    },
    "updates": {
      "fallbackToCacheTimeout": 15
    },
    "ios": {
      "buildNumber": "1.4.0",
      "bundleIdentifier": "com.slacklineus.ggby.guide2018",
      "supportsTablet": false
    },
    "android": {
      "versionCode": 5,
      "package": "com.slacklineus.ggby.guide2018",
      "permissions": []
    },
    "assetBundlePatterns": [
      "assets/**/*",
      "node_modules/@expo/vector-icons/fonts/*"
    ],
    "description": "GGBY 2019 is the 12th annual gathering of the international highline community at the Fruit Bowl over the canyons of Moab, Utah over Thanksgiving. This festival guide will help you find your way around, discover workshops, and learn how protect the Fruit Bowl for years to come.",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "hooks": {
    "postPublish": [
      {
        "file": "sentry-expo/upload-sourcemaps",
        "config": {
          "organization": "slackline-us",
          "project": "ggby-mobile"
        }
      }
    ]
  }
}
