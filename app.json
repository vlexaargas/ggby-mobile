{
  "expo": {
    "sdkVersion": "33.0.0",
    "privacy": "public",
    "name": "SlackFest",
    "icon": "./assets/appicon/SlackFestIcon.png",
    "version": "1.3.0",
    "orientation": "portrait",
    "slug": "slackfest",
    "primaryColor": "#252A33",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F4F4"
    },
    "updates": {
      "fallbackToCacheTimeout": 15
    },
    "ios": {
      "buildNumber": "1.3.0",
      "bundleIdentifier": "com.slacklineus.slackfest",
      "supportsTablet": false
    },
    "android": {
      "versionCode": 13,
      "package": "com.slacklineus.slackfest",
      "permissions": []
    },
    "assetBundlePatterns": [
      "assets/**/*",
      "node_modules/@expo/vector-icons/fonts/*"
    ],
    "description": "The guide for slackline festivals around the world."
  }
}